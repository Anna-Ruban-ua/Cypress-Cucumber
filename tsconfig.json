{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["cypress"],
    "baseUrl": "./",
    "resolveJsonModule": true
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["cypress/configs", "cypress.config.*.ts"]
}
